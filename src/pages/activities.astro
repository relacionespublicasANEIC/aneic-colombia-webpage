---
import Layout from "../layouts/Layout.astro";
import Container from "../components/Container.astro";
import SectionTitle from "../components/SectionTitle.astro";
import IconButtonBadge from "../components/IconButtonBadge.astro";
---

<Layout>
    <Container>
        <SectionTitle title="Congresos" />
    </Container>

    <Container>
        <SectionTitle title="Asambleas generales" />
    </Container>

    <Container>
        <SectionTitle
            title="Congreso Nacional de Estudiantes de Ingeniería Civil"
        />
    </Container>

    <Container>
        <IconButtonBadge
            title="Algunas de nuestras imágenes"
            description="Nuestra actividades están enfocadas para las personas"
            button="Mira la galería"
            link="#"
            icon="/ColombiaDecoration.svg"
        />
    </Container>

    <Container>
        <SectionTitle title="Jocana" />
    </Container>

    <Container>
        <SectionTitle title="Sálidas técnicas" />
    </Container>

    <Container>
        <SectionTitle title="Actividades sociales" />
    </Container>

    <Container>
        <SectionTitle
            title="Congreso Latinoamericano de Estudiantes de Ingenieria Civil"
        />
    </Container>

</Layout>
